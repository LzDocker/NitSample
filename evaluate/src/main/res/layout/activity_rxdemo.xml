<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <Button
                android:id="@+id/rxdemo_concat"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="concatMap" />

            <Button
                android:id="@+id/rxdemo_merge"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="merge" />

        </LinearLayout>


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" 总结在 EnStudyRxViewModel中 \n rx创建: interval，range，create，just \n rx变换操作符 map,flatMap,cast,merge ,concatMap,zip,buffer,filter,startWith,timeout,debounce"
            android:textSize="@dimen/common_sp_18" />


        <TextView
            android:id="@+id/rxdemo_data"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

    </LinearLayout>


</layout>