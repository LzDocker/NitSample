    <declare-styleable name="PlayPauseView">
        <attr format="color" name="bg_color"/>
        <attr format="color" name="btn_color"/>
        <attr format="float" name="gap_width"/>
        <attr format="float|reference" name="border_width"/>
        <attr format="float" name="space_padding"/>
        <attr format="integer" name="anim_duration"/>
        <attr name="anim_direction">
            <enum name="positive" value="1"/>
            <enum name="negative" value="2"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="WaveLineView">

        <attr format="color" name="wlvBackgroundColor"/>
        <attr format="color" name="wlvLineColor"/>
        <attr format="dimension" name="wlvThickLineWidth"/>
        <attr format="dimension" name="wlvFineLineWidth"/>
        <attr format="float" name="wlvMoveSpeed"/>
        <attr format="integer" name="wlvSamplingSize"/>
        <attr format="integer" name="wlvSensibility"/>
    </declare-styleable>
