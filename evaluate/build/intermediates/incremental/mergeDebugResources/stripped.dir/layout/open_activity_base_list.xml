<?xml version="1.0" encoding="utf-8"?>

                                                       
                                                  

    

                 
                            
                                                            
           

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:tag="layout/open_activity_base_list_0"
        bind:ignore="ResourceName"  xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:bind="http://schemas.android.com/tools">


        <com.docker.common.common.widget.refresh.SmartRefreshLayout
            android:id="@+id/refresh_auto"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:tag="binding_1">

            <com.docker.common.common.widget.empty.EmptyLayout
                android:id="@+id/empty_auto"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="center_vertical"
                android:gravity="center_vertical"
                android:tag="binding_2">

                <android.support.v7.widget.RecyclerView
                    android:id="@+id/recyclerView_auto"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:layoutManager="android.support.v7.widget.LinearLayoutManager"
                    app:layout_behavior="@string/appbar_scrolling_view_behavior" />
            </com.docker.common.common.widget.empty.EmptyLayout>
        </com.docker.common.common.widget.refresh.SmartRefreshLayout>


        <com.docker.common.common.widget.refresh.SmartRefreshLayout
            android:id="@+id/refresh"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <com.docker.common.common.widget.empty.EmptyLayout
                android:id="@+id/empty"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="center_vertical"
                android:gravity="center_vertical">

                <android.support.v7.widget.RecyclerView
                    android:id="@+id/recyclerView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:layoutManager="android.support.v7.widget.LinearLayoutManager"
                    app:layout_behavior="@string/appbar_scrolling_view_behavior" />


            </com.docker.common.common.widget.empty.EmptyLayout>


        </com.docker.common.common.widget.refresh.SmartRefreshLayout>
    </LinearLayout>
         


         
